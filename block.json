{
  "apiVersion": 2,
  "name": "kelsiecakes/faq-list",
  "title": "FAQ List (ACF Repeater)",
  "category": "widgets",
  "icon": "list-view",
  "description": "Outputs your FAQ ACF Repeater rows as an accessible accordion with FAQPage JSON-LD.",
  "keywords": ["faq", "accordion", "questions", "answers"],
  "supports": {
    "anchor": true,
    "align": ["wide", "full"],
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "viewScript": "file:./frontend.js",
  "style": "kelsie-faq-block",
  "editorStyle": "kelsie-faq-block-editor",
  "acf": {
    "mode": "preview",
    "renderTemplate": "render.php"
  },
  "example": {
    "attributes": {
      "preview": true
    }
  }
}
